{
  "name": "fosol-site",
  "description": "Fosol Software Solutions corporate website",
  "private": true,
  "homepage": "/",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.3.0",
  "engines": {
    "npm": ">=9.5.1 <10.0.0",
    "node": ">=18.16.0 <19.0.0",
    "yarn": ">=3.3.0 <4.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pretty-quick": "pretty-quick",
    "lint": "eslint src/ --ext .jsx,.js,.ts,.tsx --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "check": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,scss}\""
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/material": "^5.12.2",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^5.12.0",
    "@mui/styled-engine-sc": "^5.12.0",
    "@types/styled-components": "^5.1.26",
    "formik": "^2.2.9",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0",
    "react-toastify": "^9.1.2",
    "react-tooltip": "^5.11.1",
    "sort-by": "^1.2.0",
    "styled-components": "^5.3.9",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "latest",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "path": "^0.12.7",
    "prettier": "^2.8.7",
    "pretty": "^2.0.0",
    "pretty-quick": "^3.1.3",
    "sass": "^1.62.0",
    "typescript": "*",
    "vite": "^4.2.0"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^5.12.0"
  }
}
